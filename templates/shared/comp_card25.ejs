
<%
  cardMeta     = (typeof cardMeta !== 'undefined') ? cardMeta: '';
  cardTitle      = (typeof cardTitle !== 'undefined') ? cardTitle: '';
  cardDescription  = (typeof cardDescription !== 'undefined') ? cardDescription: '';
  cardList   = (typeof cardList !== 'undefined') ? cardList: '';
  ctaType   = (typeof ctaType !== 'undefined') ? ctaType: '';

%>



<div class="wrapper">
  <div class="card">
    <div class="card__left">
      <p class="card25CardMeta"><%= cardMeta %></p>
      <h2 class="card25CardTitle"><%= cardTitle %></h2>
      <h5 class="card25CardDescription"><%= cardDescription %></h5>

      <h5 class="card25CardList"><%= cardList %></h5>
      <div class="spacingLarge displayDesktop"></div>
      <div class="spacingLarge displayTablet"></div>

      <div class="spacingSmall displayMobile"></div>

      <div class="">
        <!-- Trigger 1 -->
        <a href="" class="open-modal alignBottom cta" data-modal="modal1">
          <button class="buttonMedium <%= ctaType %>">View Case Study</button>
        </a>

        <!-- Modal 1 -->
        <div id="modal1" class="modal hidden">
          <span class="modal-close">
            <img class="iconXforModal" src="../../icon/delete.svg" alt="×">
          </span>
          <div class="modal-content wrapperL">

            <div class="column element">

              <div class="containerCopy">
                <div class="ClickandCopy">Email for access.</div>
                <div class="copy-text">
                  <input id="copyEmail" name="copyEmail" type="text" class="text" value="hello.vicky.w@gmail.com" readonly>
                  <button class="centerHorz"> <img class="iconS" src="../icon/copyWt.svg">
                  </button>
                </div>
              </div>
              <h5 class="textItalic">&#160; &#160; I’ll reply with a link within 24 hours.</h5>
            </div>
          </div>
        </div>
      </div>
      <div class="spacingXSmall displayMobile"></div>





    </div>

    <div class="card__right">
      <div class="card25-media">
        <img src="/projectImages/cover_bq_inventory_management.png" alt="" class="card25-media-img" />
      </div>
    </div>
  </div>
</div>