<%
  sectionTitle     = (typeof sectionTitle !== 'undefined') ? sectionTitle: '';
  sectionDescription     = (typeof sectionDescription !== 'undefined') ? sectionDescription: '';
  spacingTop     = (typeof spacingTop !== 'undefined') ? spacingTop: '';
  spacingBottom = (spacingBottom && spacingBottom.length)
  ? spacingBottom
  : 'spacingLarge';


%>
<div class="wrapper">
  <div class="<%= spacingTop %>"></div>

  <div class="Grid Grid--full Grid--gutter">
    <div class="Grid-cell">
      <div class="element">
        <h1 class="card25SectionTitle"><%= sectionTitle %></h1>
        <h4 class="card25SectionDescription"><%= sectionDescription %></h4>
      </div>
    </div>
  </div>
  <div class="<%= spacingBottom %>"></div>

</div>
<div class="spacingXSmall"></div>