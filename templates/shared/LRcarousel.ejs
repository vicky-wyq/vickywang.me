<% if (slide === '2') { %>
    <div class="blankHXS"></div>
    <div class="wrapper">
      <div class="Grid-cell">
        <h2 class="element"><%= slideshowTitle %> </h2>
      </div>
    </div>
    <div class="wrapper">
      <div class="LRcarousel Grid Grid--gutters Grid--1of4 Grid--center u-textCenter" aria-label="LRcarousel" Tabindex="0" id="carousel1">
        <div class="carousel__nav Grid-cell ">
    
          <a class="slider-nav" aria-label="Go to slide 1">
            <div> <%= contentTitle1 %> </div>
          </a>
          <a class="slider-nav" aria-label="Go to slide 2">
            <div><%= contentTitle2 %></div>
          </a>

        </div>
        <div class="LRslides Grid-cell">
          <div class="slides-item slide-1" id="slide-1" aria-label="slide 1 of 3" tabindex="0">
            <img src="<%= img1 %>" alt="">
          </div>
          <div class="slides-item slide-2" id="slide-2" aria-label="slide 2 of 3" tabindex="0">
            <img src="<%= img2 %>" alt="">
    
          </div>

        </div>
      </div>
    </div>
    <div class="blankHXS"></div>



