<% if (typeof descriptionType === 'undefined') { 
  descriptionType = ''; 
} 
%>




<% if (typeof subTitleFont1 === 'undefined') { 
    subTitleFont1 = ''; 
  } 
  %>
<% if (typeof subTitleFont2 === 'undefined') { 
  subTitleFont2 = ''; 
} 
%>
<% if (typeof subTitle1 === 'undefined') { 
  subTitle1 = ''; 
} 
%>
<% if (typeof subTitle2 === 'undefined') { 
  subTitle2 = ''; 
} 
%>

<% if (typeof paragraphFont === 'undefined') { 
    paragraphFont = ''; 
  } 
  %>
<% if (typeof paragraphTxt1 === 'undefined') { 
    paragraphTxt1 = ''; 
  } 
  %>
<% if (typeof paragraphTxt2 === 'undefined') { 
  paragraphTxt2 = ''; 
} 
%>
<% if (typeof paragraphTxt3 === 'undefined') { 
  paragraphTxt3 = ''; 
} 
%>
<% if (typeof paragraphTxt4 === 'undefined') { 
  paragraphTxt4 = ''; 
} 
%>

<% if (typeof fontColor1 === 'undefined') { 
  fontColor1 = ''; 
} 
%>

<% if (typeof fontColor2 === 'undefined') { 
  fontColor2 = ''; 
} 
%>


<% if (typeof listTxt1 === 'undefined') { 
  listTxt1 = ''; 
} 
%>
<% if (typeof listTxt2 === 'undefined') { 
  listTxt2 = ''; 
} 
%>
<% if (typeof listTxt3 === 'undefined') { 
  listTxt3 = ''; 
} 
%>
<% if (typeof listTxt4 === 'undefined') { 
  listTxt4 = ''; 
} 
%>
<!-- subTitleFont1 -->
<div class="Grid-cell">
  <div class="element">
    <% if (subTitleFont1==='h2' ) { %>
    <h2>
      <strong class="<%= fontColor1 %>">
        <%= subTitle1 %>
      </strong>
    </h2>
    <% } else if (subTitleFont1==='h3' ) { %>
    <h3>
      <strong class="<%= fontColor1 %>">

        <%= subTitle1 %>
      </strong>
    </h3>
    <% } else if (subTitleFont1==='h4' ) { %>
    <h4>
      <strong class="<%= fontColor1 %>">
        <%= subTitle1 %>
      </strong>
    </h4>
    <% } %>


    <!-- subTitleFont2 -->

    <% if (subTitleFont2==='h2' ) { %>
    <h2>
      <strong class="<%= fontColor2 %>">
        <%= subTitle2 %>
      </strong>
    </h2>
    <% } else if (subTitleFont2==='h3' ) { %>
    <h3>
      <strong class="<%= fontColor2 %>">

        <%= subTitle2 %>
      </strong>
    </h3>
    <% } else if (subTitleFont2==='h4' ) { %>
    <h4>
      <strong class="<%= fontColor2 %>">
        <%= subTitle2 %>
      </strong>
    </h4>
    <% } %>


    <!-- paragraphFont -->
    <% if (descriptionType === 'paragraph' ) { %>


      <% if (paragraphFont==='h2' ) { %>
      <h2>
        <%= paragraphTxt1 %>
      </h2>

      <h2>
        <%= paragraphTxt2 %>
      </h2>
      <h2>
        <%= paragraphTxt3 %>
      </h2>
      <h2>
        <%= paragraphTxt4 %>
      </h2>

      <% } else if (paragraphFont==='h3' ) { %>
      <h3>
        <%= paragraphTxt1 %>
      </h3>
      <h3>
        <%= paragraphTxt2 %>
      </h3>
      <h3>
        <%= paragraphTxt3 %>
      </h3>
      <h3>
        <%= paragraphTxt4 %>
      </h3>
      <% } else if (paragraphFont==='h4' ) { %>
      <h4>
        <%= paragraphTxt1 %>
      </h4>
      <h4>
        <%= paragraphTxt2 %>
      </h4>
      <h4>
        <%= paragraphTxt3 %>
      </h4>
      <h4>
        <%= paragraphTxt4 %>
      </h4>
      <% } else if (paragraphFont==='h5' ) { %>
      <h5>
        <%= paragraphTxt1 %>
      </h5>
      <h5>
        <%= paragraphTxt2 %>
      </h5>
      <h5>
        <%= paragraphTxt3 %>
      </h5>
      <h5>
        <%= paragraphTxt4 %>
      </h5>
      <% } else if (paragraphFont==='h6' ) { %>
      <h6>
        <%= paragraphTxt1 %>
      </h6>
      <h6>
        <%= paragraphTxt2 %>
      </h6>
      <h6>
        <%= paragraphTxt3 %>
      </h6>
      <h6>
        <%= paragraphTxt4 %>
      </h6>
      <% } %>

    
    <% } else if (descriptionType ==='list' ) { %>
      <ul>
        <% if (listTxt1 === '' ) { %>
        <% } else { %>
        <li class="listCircle"><%= listTxt1 %></li>
        <% } %>
  
  
        <% if (listTxt2 === '' ) { %>
        <% } else { %>
        <li class="listCircle"><%= listTxt2 %></li>
        <% } %>
  
        <% if (listTxt3 === '' ) { %>
        <% } else { %>
        <li class="listCircle"><%= listTxt3 %></li>
        <% } %>
  
  
        <% if (listTxt4 === '' ) { %>
        <% } else { %>
        <li class="listCircle"><%= listTxt4 %></li>
        <% } %>
      </ul>
    <% } else { %>

    <% } %>


  </div>
</div>