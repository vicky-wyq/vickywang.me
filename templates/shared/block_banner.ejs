  bannerSubTitle = ''; 
} 
%>
<% if (typeof phoneBanner === 'undefined') { 
  phoneBanner = ''; 
} 
%>
  <div class="<%= bannerBgColor %>">
    <div class="blankPhone"></div>
    <% if (phoneBanner === '' ) { %>
      <div class="blankHS"></div>
    <% } else { %>
    <% } %>


    <div class="ProjectBanner displayLaptop">
      <% if (phoneBanner === '' ) { %>
      <div class="blankMin1880"></div>
      <% } else { %>
      <% } %>
