<% if (typeof bgColor === 'undefined') { 
  bgColor = ''; 
} 
%>

<% if (typeof SlideDeckUrl === 'undefined') { 
  SlideDeckUrl = ''; 
} 
%>
<% if (typeof bannerLogoUrl === 'undefined') { 
  bannerLogoUrl = ''; 
} 
%>
<% if (typeof bannerTitle === 'undefined') { 
  bannerTitle = ''; 
} 
%>
